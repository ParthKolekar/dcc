class Program {
  int get_int(int x) {
    return x;
  }

  boolean get_bool(boolean x) {
    return x;
  }

  int foo() {
    int a, b, c;
    boolean x;

    a = get_int(7);
    b = get_int(2);
    x = get_bool(true);
    c = 0;

    c = a + b;
    if (x) {
      return a + b;
    }
    
    return c - 3;
  }

  void main() {
    int c;
    c = foo();
    callout("printf", "%d\n", c);
  }
}
