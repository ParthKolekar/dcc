// Assuming fibonacci series starts from 1 for n=1

class Program 
{
	int fibo(int N)
	{
        if (N==0||N==1) {
			return N;
        }
        return fibo(N - 1) + fibo(N - 2);
	}
	void  main()
	{
		int n;
		int a;
		n = 10;
		callout("printf","%d ",n);
		a=fibo(n);
		callout("printf","%d ",a);
	}
}
